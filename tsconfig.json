{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "./src"
    // "paths": {
    //   "@/*": ["./src/*"]
    // }
  },
  // "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/pages/404.js"],
  "include": ["src", "public/assets"],
  "exclude": ["node_modules"]
}
